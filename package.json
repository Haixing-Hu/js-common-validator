{
  "name": "@haixing_hu/common-validator",
  "version": "3.1.1",
  "description": "A JavaScript ES6 library for common fields validators",
  "author": "Haixing Hu (starfish.hu@gmail.com)",
  "license": "Apache-2.0",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Haixing-Hu/js-common-validator.git"
  },
  "bugs": {
    "url": "https://github.com/Haixing-Hu/js-common-validator/issues"
  },
  "homepage": "https://github.com/Haixing-Hu/js-common-validator",
  "keywords": [
    "JavaScript",
    "validator"
  ],
  "main": "dist/common-validator.cjs.min.js",
  "module": "dist/common-validator.esm.min.mjs",
  "exports": {
    ".": {
      "require": "./dist/common-validator.cjs.min.js",
      "import": "./dist/common-validator.esm.min.mjs"
    }
  },
  "sideEffects": false,
  "scripts": {
    "clean": "rm -rvf dist doc",
    "build": "cross-env NODE_ENV=production rollup -c rollup.config.mjs",
    "build:dev": "cross-env NODE_ENV=development rollup -c rollup.config.mjs",
    "build:all": "yarn clean && yarn doc && yarn build:dev && yarn build",
    "test": "jest",
    "lint": "eslint ./src ./test",
    "doc": "jsdoc -c jsdoc.json",
    "es5": "check-es-version -e 5 -s true",
    "deploy": "yarn && yarn lint && yarn test && yarn build:all && npm publish --registry='https://registry.npmjs.com/' --access public"
  },
  "dependencies": {
    "email-validator": "^2.0.4",
    "node-uri": "^1.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.23.2",
    "@babel/eslint-parser": "^7.17.0",
    "@babel/plugin-proposal-decorators": "^7.23.2",
    "@babel/plugin-transform-class-properties": "^7.22.5",
    "@babel/plugin-transform-runtime": "^7.23.2",
    "@babel/preset-env": "^7.23.2",
    "@babel/runtime": "^7.23.2",
    "@haixing_hu/eslint-config": "^1.3.1",
    "@haixing_hu/jsdoc-minami": "^1.5.1",
    "@haixing_hu/rollup-builder": "^1.5.0",
    "@jest/core": "^29.7.0",
    "@rollup/plugin-alias": "^5.0.1",
    "@rollup/plugin-babel": "^6.0.4",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "babel-jest": "^29.7.0",
    "babel-plugin-transform-object-hasown": "^1.1.0",
    "check-es-version": "^1.4.2",
    "core-js": "^3.33.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.49.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-environment-jsdom-global": "^4.0.0",
    "jest-extended": "^4.0.1",
    "jsdoc": "^4.0.2",
    "rollup": "^4.1.3",
    "rollup-plugin-analyzer": "^4.0.0"
  },
  "packageManager": "yarn@4.0.0"
}
